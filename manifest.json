{
"update_url": "https://clients2.google.com/service/update2/crx",

	"name":"Box Appid Switcher",
	"short_name": "Appid Switch",
	"version": "1.40",
	"description": "A simple extensions that changes all of the appids in the current window",
	"permissions": ["tabs",
					"declarativeContent",
					"https://boxhomeloans.nanolos.com/*",
					"https://flow.nanolos.com/*",
					"https://docs.nanolos.com/*"],
	"background": {
		"scripts": ["background.js"],
		"persistent": false
	},
	"browser_action": {
		"default_popup": "popup.html",
		"default_icon": {
        "16": "images/Box_16.png",
        "32": "images/Box_32.png",
        "48": "images/Box_48.png",
		"64": "images/Box_64.png",
        "128":"images/Box_128.png"
      }
	},
	"icons": {
		"16": "images/Box_16.png",
        "32": "images/Box_32.png",
        "48": "images/Box_48.png",
		"64": "images/Box_64.png",
        "128":"images/Box_128.png"
    },
	"manifest_version": 2
}
